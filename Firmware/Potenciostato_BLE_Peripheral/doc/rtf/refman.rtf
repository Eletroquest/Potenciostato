148               vTaskDelay(metodo_r.metodo.cv.rate / portTICK_PERIOD_MS);\par
149               LMP_readVoltage(j);\par
150               send_data.output.cv = data_CV_f;\par
151               send_data.type = metodo_r.type;\par
152               xQueueSend(output_queue, &send_data, 0);\par
153               {\cf20 // print_data_CV();}\par
154             \}\par
155           \}\par
156         \}\par
157         send_data.type = Generic_type;\par
158         strcpy(send_data.output.string, {\cf22 "end_CV\\n"});\par
159         xQueueSend(output_queue, &send_data, 0);\par
160       \}\par
161       {\cf19 else} {\cf19 if} (metodo_r.type == Amp_metodo)\par
162       \{\par
163         {\cf19 if} (metodo_r.metodo.amp.range == 12)\par
164           current = {\cf22 "Current(pA)"};\par
165         {\cf19 else} {\cf19 if} (metodo_r.metodo.amp.range == 9)\par
166           current = {\cf22 "Current(nA)"};\par
167         {\cf19 else} {\cf19 if} (metodo_r.metodo.amp.range == 6)\par
168           current = {\cf22 "Current(uA)"};\par
169         {\cf19 else} {\cf19 if} (metodo_r.metodo.amp.range == 3)\par
170           current = {\cf22 "Current(mA)"};\par
171         {\cf19 else}\par
172           current = {\cf22 "SOME ERROR"};\par
173 \par
174         initLMP(metodo_r.metodo.amp.lmpGain);\par
175 \par
176         int16_t voltageArray[3] = \{metodo_r.metodo.amp.pre_stepV, metodo_r.metodo.amp.v1, metodo_r.metodo.amp.v2\};\par
177         uint32_t timeArray[3] = \{metodo_r.metodo.amp.quietTime, metodo_r.metodo.amp.t1, metodo_r.metodo.amp.t2\};\par
178 \par
179         {\cf20 // Envia cabe\'E7alho do metodo}\par
180         send_data.type = Generic_type;\par
181         std::string saida_cp = {\cf22 "Voltage(mV)\\tTime(ms)\\t"} + current + {\cf22 "\\n"};\par
182         strcpy(send_data.output.string, saida_cp.c_str());\par
183         xQueueSend(output_queue, &send_data, 0);\par
184 \par
185         {\cf19 for} (uint8_t i = 0; i < 3; i++)\par
186         \{\par
187           {\cf20 // o tempo entre as amostras \'E9 determinado pelo}\par
188           {\cf20 // n\'FAmero de amostras inserido pelo usu\'E1rio}\par
189           uint32_t fs = (double)timeArray[i] / metodo_r.metodo.amp.samples;\par
190           {\cf18 unsigned} {\cf18 long} startTime = millis();\par
191 \par
192           LMP_setVoltage(voltageArray[i]);\par
193 \par
194           {\cf19 while} (millis() - startTime < timeArray[i])\par
195           \{\par
196             readData(&dataStruct);\par
197 \par
198             {\cf18 float} v1 = 2 * vref * (float(dataStruct.channel1) / (pow(2, 24)));\par
199             {\cf18 float} v2 = 2 * vref * (float(dataStruct.channel2) / (pow(2, 24)));\par
200             {\cf18 float} current = 0;\par
201 \par
202             {\cf19 if} (LMPgain == 0)\par
203               current = (((v1 - v2) / 1000) / RFB) * pow(10, metodo_r.metodo.amp.range); {\cf20 // scales to nA}\par
204             {\cf19 else}\par
205               current = (((v1 - v2) / 1000) / TIA_GAIN[LMPgain - 1]) * pow(10, metodo_r.metodo.amp.range); {\cf20 // scales to nA}\par
206 \par
207             {\cf18 float} current2 = ((((v2 - v1)) / (TIA_GAIN[LMPgain - 1])));\par
208 \par
209             {\cf20 // seta para imprimir os dados}\par
210             data_Amp_f.voltage = voltageArray[i];\par
211             data_Amp_f.time = millis();\par
212             data_Amp_f.current = current;\par
213             send_data.output.amp = data_Amp_f;\par
214             send_data.type = metodo_r.type;\par
215             xQueueSend(output_queue, &send_data, 0);\par
216 \par
217             vTaskDelay((fs - 1) / portTICK_PERIOD_MS);\par
218           \}\par
219         \}\par
220 \par
221         send_data.type = Generic_type;\par
222         strcpy(send_data.output.string, {\cf22 "end_Amp\\n"});\par
223         xQueueSend(output_queue, &send_data, 0);\par
224       \}\par
225       vTaskDelay(200 / portTICK_PERIOD_MS);\par
226       inicia_blink = {\cf17 false};\par
227     \}\par
228   \}\par
229 \}\par
}
