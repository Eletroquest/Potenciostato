\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 297 \{\par
298   {\cf20 // Tens\'E3o DAC m\'EDnima que pode ser definida}\par
299   {\cf20 // O LMP91000 aceita um valor m\'EDnimo de 1,5V, adicionando os}\par
300   {\cf20 // 20 mV adicionais para ter uma margem de seguran\'E7a}\par
301   {\cf17 const} uint16_t minDACVoltage = 1520;\par
302   bias_setting = 0;\par
303 \par
304   {\cf19 if} (voltage == 0)\par
305   \{\par
306     dacVout = 1500;\par
307   \}\par
308   {\cf19 else}\par
309   \{\par
310     dacVout = minDACVoltage;\par
311     {\cf20 // a tens\'E3o n\'E3o pode ser ajustada para menos de 15mV, porque o LMP91000}\par
312     {\cf20 // aceita um m\'EDnimo de 1,5V no pino VREF e possui 1% como sua}\par
313     {\cf20 // menor op\'E7\'E3o de polariza\'E7\'E3o 1,5V * 1% = 15mV}\par
314     {\cf19 if} (abs(voltage) < 15)\par
315       voltage = 15 * (voltage / abs(voltage));\par
316 \par
317     int16_t setV = dacVout * TIA_BIAS[bias_setting];\par
318     voltage = abs(voltage);\par
319 \par
320     {\cf19 while} (setV > voltage * (1 + v_tolerance) || setV < voltage * (1 - v_tolerance))\par
321     \{\par
322       {\cf19 if} (bias_setting == 0)\par
323         bias_setting = 1;\par
324 \par
325       dacVout = voltage / TIA_BIAS[bias_setting];\par
326 \par
327       {\cf19 if} (dacVout > opVolt)\par
328       \{\par
329         bias_setting++;\par
330         dacVout = 1500;\par
331         {\cf19 if} (bias_setting > NUM_TIA_BIAS)\par
332         \{\par
333           bias_setting = 0;\par
334         \}\par
335       \}\par
336 \par
337       setV = dacVout * TIA_BIAS[bias_setting];\par
338     \}\par
339   \}\par
340   xSemaphoreTake(xSemaphore_writeLMP, portMAX_DELAY);\par
341   pStat.setBias(bias_setting);\par
342   dac_set_vout(&dac_f, dacVout);\par
343   xSemaphoreGive(xSemaphore_writeLMP);\par
344 \par
345   data_CV_f.zero = dacVout;\par
346   data_CV_f.bias = TIA_BIAS[bias_setting];\par
347 \}\par
}
}
