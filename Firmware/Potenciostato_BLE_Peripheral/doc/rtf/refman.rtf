\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 259 \{\par
260   {\cf20 // tens\'E3o de sa\'EDda do amplificador transimped\'E2ncia}\par
261   readData(&dataStruct);\par
262 \par
263   {\cf18 float} v1 = 0;\par
264   {\cf19 if} (IntZ == 3)\par
265     v1 = (float(dacVout) / 1000);\par
266   {\cf19 else}\par
267     v1 = (float(dacVout) / 1000) * TIA_ZERO[IntZ];\par
268   {\cf18 float} v2 = 2 * vref * (float(dataStruct.channel2) / (pow(2, 24)));\par
269   {\cf18 float} current = 0;\par
270   {\cf20 // a corrente \'E9 determinada pelo zero do amplificador transimped\'E2ncia}\par
271   {\cf20 // a partir da sa\'EDda do amplificador transimped\'E2ncia, e depois dividindo}\par
272   {\cf20 // pelo resistor de feedback}\par
273   {\cf20 // corrente = (V_OUT - V_IN) / RFB}\par
274   {\cf20 // v1 e v2 est\'E3o em milivolts}\par
275   {\cf19 if} (LMPgain == 0)\par
276     current = (((v2 - v1) / 1000) / RFB) * pow(10, 9); {\cf20 // escala para nA}\par
277   {\cf19 else}\par
278     current = (((v2 - v1) / 1000) / TIA_GAIN[LMPgain - 1]) * pow(10, 9); {\cf20 // escalado para uA pow(10,6)=uA ou pow(10,9)=nA}\par
279 \par
280   {\cf18 float} current2 = ((((v2 - v1)) / (TIA_GAIN[LMPgain - 1])));\par
281   \par
282   data_CV_f.vout = v2;\par
283   data_CV_f.c1 = v1;\par
284   data_CV_f.current = current;\par
285   data_CV_f.current2 = current2 * pow(10, 6);\par
286   data_CV_f.voltage_f = voltage;\par
287   data_CV_f.current_f = current;\par
288 \}\par
